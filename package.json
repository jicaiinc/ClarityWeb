    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "svgo": "^3.0.2",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "sidePanel",
      "storage",
      "unlimitedStorage",
      "tabs",
      "scripting"
    ],
    "externally_connectable": {
      "matches": [
        "*://localhost/*",
        "*://127.0.0.1/*"
      ]
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "default_locale": "en"
  }
